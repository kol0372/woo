<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.1.xsd">
	<!-- 어노테이션 설정 -->
	<!-- <context:annotation-config /> 이걸 모든빈 위에 써야 어노테이션이 설정완료 -->
	<context:annotation-config />


	<bean id="name1" class="java.lang.String">
	<!-- <qualifier value=""></qualifier> 여러개 있을때 얘로 판별가능 -->
		<qualifier value="hold"></qualifier>
		<constructor-arg value="김길동"></constructor-arg>
	</bean>

	<bean id="name2" class="java.lang.String">
		<constructor-arg value="이지영"></constructor-arg>
	</bean>


	<bean id="auto" class="ex1Auto.AutoWiredService"></bean>

</beans>
