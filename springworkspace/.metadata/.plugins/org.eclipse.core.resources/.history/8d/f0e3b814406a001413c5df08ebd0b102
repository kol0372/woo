package order;

public class OrderSystem {

	private YourOrder menu;

	public String printMenu(YourOrder menu) {
		MyOrder myorder = new MyOrder();
		
		String re = myorder.order(menu);
		return re;
	}

	public YourOrder getMenu() {
		return menu;
	}

	public void setMenu(YourOrder menu) {
		this.menu = menu;
	};
	
	
}
