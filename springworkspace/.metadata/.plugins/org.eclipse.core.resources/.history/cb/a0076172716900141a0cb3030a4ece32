package ex1;

public class Ex2ServiceImple implements Service {

	private int num;
	private String name, str;

	public Ex2ServiceImple(int num) {
		
		this.num = num;
	}

	public Ex2ServiceImple(String str) {
		
		this.str = str;
	}

	@Override
	public void print() {
		System.out.println("NUM" + (num+10));

	}

	@Override
	public void call() {
		System.out.println("STR" + str +"¢¾");


	}

	@Override
	public void moniter() {
		System.out.println("NUM" + (num+10));
		System.out.println("STR" + str +"¢¾");

	}

}
